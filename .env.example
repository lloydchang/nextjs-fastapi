# .env.example

# ==========================
# Environment Variable Example Configuration
# ==========================

# ==========================
# Instructions:
# 1. Copy this file as `.env.example` in your project root, such as `cp .env.example .env.local`.
# 2. Replace 'your_SERVICE_api_key_here' with your actual API key.
    
# API keys for various services
    
# Anthropic API key
NEXT_PUBLIC_ANTHROPIC_API_KEY=your_anthropic_api_key_here
NEXT_PUBLIC_ANTHROPIC_API_BASE_URL=https://api.anthropic.com/v1/complete
# Obtain from: https://www.anthropic.com/
    
# AWS API key
NEXT_PUBLIC_AWS_API_KEY=your_aws_api_key_here
NEXT_PUBLIC_AWS_API_BASE_URL=https://bedrock.aws.com/v1/completions
# Obtain from: https://aws.amazon.com/console/
    
# Azure API key
NEXT_PUBLIC_AZURE_API_KEY=your_azure_api_key_here
NEXT_PUBLIC_AZURE_API_BASE_URL=https://api.openai.azure.com/v1/completions
# Obtain from: https://azure.microsoft.com/en-us/products/cognitive-services/openai-service/
    
# Cohere API key
NEXT_PUBLIC_COHERE_API_KEY=your_cohere_api_key_here
NEXT_PUBLIC_COHERE_API_BASE_URL=https://api.cohere.ai/v1/generate
# Obtain from: https://cohere.ai/
    
# Google API key
NEXT_PUBLIC_GOOGLE_API_KEY=your_google_api_key_here
NEXT_PUBLIC_GOOGLE_API_BASE_URL=https://api.google.com/v1
# Obtain from: https://console.cloud.google.com/
    
# Groq API key
NEXT_PUBLIC_GROQ_API_KEY=your_groq_api_key_here
NEXT_PUBLIC_GROQ_API_BASE_URL=https://api.groq.com/v1/generate
# Obtain from: https://groq.com/
    
# Hugging Face API key
NEXT_PUBLIC_HUGGINGFACE_API_KEY=your_huggingface_api_key_here
NEXT_PUBLIC_HUGGINGFACE_API_BASE_URL=https://api-inference.huggingface.co/models/gpt
# Obtain from: https://huggingface.co/
    
# Ollama does not require an API key, but you can specify it if needed
NEXT_PUBLIC_OLLAMA_API_KEY=ollama
NEXT_PUBLIC_OLLAMA_API_BASE_URL=http://localhost:11434/
# Obtain from: https://ollama.com/
    
# OpenAI API key
NEXT_PUBLIC_OPENAI_API_KEY=your_openai_api_key_here
NEXT_PUBLIC_OPENAI_API_BASE_URL=https://api.openai.com/v1/completions
# Obtain from: https://platform.openai.com/
    
# OpenRouter API key
NEXT_PUBLIC_OPENROUTER_API_KEY=your_openrouter_api_key_here
NEXT_PUBLIC_OPENROUTER_API_BASE_URL=https://openrouter.ai/api/v1/completions
# Obtain from: https://openrouter.ai/
    
# Oracle API key
NEXT_PUBLIC_ORACLE_API_KEY=your_oracle_api_key_here
NEXT_PUBLIC_ORACLE_API_BASE_URL=https://api.oracle.com/v1/generative-ai
# Obtain from: https://www.oracle.com/solutions/ai/
